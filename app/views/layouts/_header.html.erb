<header>
  <img src="logo.png" name="menu" width="290" height="89" border="0">
  <nav class="round">
    <ul>
      <li><%= link_to "Home", root_path %></li>
      <% if signed_in? %>
      <li><%= link_to "Calendar", calendar_index_path %></li>
      <li><%= link_to "Account Management", edit_user_path(current_user) %></li>
      <li><%= link_to "Sign out", signout_path, :method => :delete %></li>
      <% else %>
      <li><%= link_to "Sign in", signin_path %></li>
      <% end %>
    </ul>
  </nav>
</header>

